.model 4bit_adder
.inputs A3 A2 A1 A0 B3 B2 B1 B0 CIN
.outputs COUT S3 S2 S1 S0
.subckt full_adder a=A0 b=B0 cin=CIN sum=S0 cout=CARRY1
.subckt full_adder a=A1 b=B1 cin=CARRY1 sum=S1 cout=CARRY2
.subckt full_adder a=A2 b=B2 cin=CARRY2 sum=S2 cout=CARRY3
.subckt full_adder a=A3 b=B3 cin=CARRY3 sum=S3 cout=COUT
.end

.model full_adder
.inputs a b cin
.outputs sum cout
.names a b cin sum #sum=XOR(a,b,cin)
100 1
010 1
001 1
111 1
.names a b cin cout #c-out=MAJ(a,b,cin)
11- 1
1-1 1
-11 1
.end
